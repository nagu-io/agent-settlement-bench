{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AgentSettlementBench Manual Run Summary",
  "type": "object",
  "required": [
    "schema_version",
    "model",
    "run_type",
    "valid_for_leaderboard",
    "coverage_basis",
    "source",
    "date",
    "is_estimated",
    "cases_evaluated",
    "benchmark_total_cases",
    "benchmark_coverage_pct",
    "pass",
    "fail",
    "pass_rate_pct",
    "critical_cases_evaluated",
    "critical_fail_count",
    "critical_fail_rate_pct",
    "risk_weighted_fail_rate_pct",
    "risk_weighted_accuracy_pct"
  ],
  "properties": {
    "schema_version": { "type": "integer", "const": 1 },
    "model": { "type": "string", "minLength": 1 },
    "run_type": { "type": "string", "const": "manual_sample" },
    "valid_for_leaderboard": { "type": "boolean", "const": false },
    "coverage_basis": { "type": "string", "minLength": 1 },
    "source": { "type": "string", "minLength": 1 },
    "date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
    "is_estimated": { "type": "boolean" },
    "decisions_file": { "type": "string" },
    "cases_evaluated": { "type": "integer", "minimum": 1 },
    "benchmark_total_cases": { "type": "integer", "minimum": 1 },
    "benchmark_coverage_pct": { "type": "number", "minimum": 0, "maximum": 100 },
    "pass": { "type": "integer", "minimum": 0 },
    "fail": { "type": "integer", "minimum": 0 },
    "pass_rate_pct": { "type": "number", "minimum": 0, "maximum": 100 },
    "critical_cases_evaluated": { "type": "integer", "minimum": 0 },
    "critical_fail_count": { "type": "integer", "minimum": 0 },
    "critical_fail_rate_pct": { "type": "number", "minimum": 0, "maximum": 100 },
    "risk_weighted_fail_rate_pct": { "type": "number", "minimum": 0, "maximum": 100 },
    "risk_weighted_accuracy_pct": { "type": "number", "minimum": 0, "maximum": 100 },
    "notes": { "type": "string" }
  },
  "additionalProperties": true
}
